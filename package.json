{
  "name": "@unitz/graphme",
  "version": "1.0.3",
  "description": "GraphQL query builder lib",
  "private": false,
  "main_dist": "dist/graphme.cjs.js",
  "module_dist": "dist/graphme.esm.js",
  "browser_dist": "dist/graphme.umd.js",
  "keywords": [
    "graphql",
    "builder",
    "orm",
    "model"
  ],
  "homepage": "https://github.com/Unitz-Co/graphme.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/Unitz-Co/graphme.git"
  },
  "author": "Hung Tran <hungtran0203@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "watch": "concurrently 'yarn build:watch' 'yarn serve -l tcp://127.0.0.1'",
    "clean": "rimraf ./dist",
    "mock:up": "docker-compose -f ./scripts/mock/docker-compose.yaml up",
    "mock:down": "docker-compose -f ./scripts/mock/docker-compose.yaml down",
    "mock:clean": "yarn mock:clean:container && yarn mock:clean:volume",
    "mock:clean:container": "docker ps -a | grep 'mock_test_*' | awk '{print $1}' | xargs docker rm",
    "mock:clean:volume": "docker volume ls -q | grep 'mock_*' | awk '{print $1}' | xargs docker volume rm",
    "mock:wait": "yarn mock:up -d && dockerize -wait tcp://localhost:28080 -timeout 15m",
    "test": "jest --runInBand",
    "test:u": "jest --runInBand --updateSnapshot",
    "test:watch": "jest --watch --coverage",
    "coverage": "jest --collectCoverageFrom=./**.js --coverage ./",
    "preversion": "yarn test",
    "postversion": "git push --tags && yarn publish . --tag $npm_package_version && git push && echo \"Successfully released version $npm_package_version!\""
  },
  "dependencies": {
    "@babel/runtime": "^7.7.0",
    "@unitz/gqlbuilder": "^1.0.2",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.0",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-decorators": "^7.7.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.0",
    "@babel/plugin-syntax-dynamic-import": "^7.7.0",
    "@babel/plugin-transform-destructuring": "^7.7.0",
    "@babel/plugin-transform-for-of": "^7.7.0",
    "@babel/plugin-transform-regenerator": "^7.7.0",
    "@babel/plugin-transform-runtime": "^7.7.0",
    "@babel/preset-env": "^7.7.0",
    "@loadable/babel-plugin": "^5.10.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@trendyol/jest-testcontainers": "^2.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "24.9.0",
    "babel-plugin-macros": "^3.0.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "concurrently": "4.1.2",
    "eslint": "^7.8.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-autofix": "^1.0.2",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^22.21.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-unused-imports": "^1.0.0",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "graphql-subscriptions-client": "^0.16.0",
    "graphql-tag": "^2.9.2",
    "jest": "^26.5.3",
    "jest-fetch-mock": "^3.0.3",
    "jest-junit": "^12.0.0",
    "jest-runner-eslint": "^0.10.0",
    "lint-staged": "^10.4.2",
    "rollup": "^2.30.0",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-visualizer": "^4.2.0",
    "serve": "11.3.2",
    "ws": "^7.4.0"
  }
}
